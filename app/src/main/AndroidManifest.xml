<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.acktos.conductorvip"
    android:versionCode="17"
    android:versionName="2.0.1" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="com.acktos.conductorvip.permission.C2D_MESSAGE" />
    <uses-permission android:name="com.google.android.c2dm.permission.SEND" />


    <!-- Maps API needs OpenGL ES 2.0. -->
    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <!-- DEBUG API KEY -->
        <!--
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyAZfHdKhmvQi5s2yPST2WPXndpbUZuZ92k" />-->
       

        <!-- PRODUCTION API KEY -->

        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyC-WgGf4iAxrQjPmYjCO67SwUeFrvXc1Vg" />

        <activity
            android:name="com.acktos.conductorvip.MapServiceActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>

        <activity
            android:name="com.acktos.conductorvip.LoginActivity"
            android:label="@string/title_activity_login"
            android:windowSoftInputMode="adjustResize|stateVisible" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.acktos.conductorvip.ResultServiceActivity"
            android:label="@string/title_activity_result_service"
            android:noHistory="true" >
        </activity>
        <activity
            android:name="com.acktos.conductorvip.PendingServicesActivity"
            android:label="@string/title_activity_pending_services" >
        </activity>
        <activity
            android:name="com.acktos.conductorvip.BillDetailActivity"
            android:label="@string/title_activity_bill_detail" >
        </activity>
        <activity
            android:name="com.acktos.conductorvip.ServiceDetailActivity"
            android:label="@string/title_activity_service_detail" >
        </activity>

        <service android:name="com.acktos.conductorvip.services.ServiceTest" />
        <service android:name="com.acktos.conductorvip.services.TrackService" />
        <service android:name="com.acktos.conductorvip.services.ChosenDriverService" android:exported="false" ></service>
        <service android:name="com.acktos.conductorvip.services.AcceptIntentService" android:exported="false" ></service>
        <service android:name="com.acktos.conductorvip.services.AssignDriverService" android:exported="false" ></service>
        <service android:name="com.acktos.conductorvip.services.GcmIntentService" />


        <receiver
            android:name="com.acktos.conductorvip.broadcast.GcmBroadcastReceiver"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />

                <category android:name="com.acktos.conductorvip" />
            </intent-filter>
        </receiver>


    </application>

</manifest>